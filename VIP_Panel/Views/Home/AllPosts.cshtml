@page
@model List<PostModel>

<table>
<tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>

</tr>
@foreach (var item in Model)
{
<tr>
    <td>
        @item.ID
    </td>
    <td>
        @item.Content
    </td>
    <td>
        @item.Date
    </td>
    <td>
        @item.UserID
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
        @Html.ActionLink("Details", "Details", new { id = item.ID })  |
        @Html.ActionLink("Delete", "Delete", new { id = item.ID })
    </td>
</tr>}
</table>

